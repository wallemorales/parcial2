select EMPLEADOS.NOMBRES as NOMBRES,
    CARGO.CARGO as CARGO 
 from CARGO CARGO,
    EMPLEADOS EMPLEADOS



select EMPLEADOS.NOMBRES as NOMBRES,
    CARGO.CARGO as CARGO,
    ESTRATO.ESTRATO as ESTRATO,
    SEDE.ID_SEDE as ID_SEDE 
 from SEDE SEDE,
    ESTRATO ESTRATO,
    CARGO CARGO,
    EMPLEADOS EMPLEADOS
    
    
    select EMPLEADOS.NOMBRES as NOMBRES,
    EMPLEADOS.FECHA_DE_CONTRATACION as FECHA_DE_CONTRATACION,
    CARGO.CARGO as CARGO,
    ESTRATO.ESTRATO as ESTRATO 
 from SEDE SEDE,
    ESTRATO ESTRATO,
